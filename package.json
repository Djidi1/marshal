{
  "name": "marshal-service",
  "version": "1.0.8",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "axios": "^0.18.1",
    "browserslist": "^4.7.0",
    "caniuse-lite": "^1.0.30000989",
    "framework7": "^4.5.0",
    "framework7-icons": "^2.3.1",
    "framework7-react": "^4.5.0",
    "idb-keyval": "^3.2.0",
    "intro.js": "^2.9.3",
    "intro.js-react": "^0.2.0",
    "lodash": "^4.17.15",
    "material-design-icons": "^3.0.1",
    "react": "^16.9.0",
    "react-detect-offline": "^2.4.0",
    "react-dom": "^16.9.0",
    "react-images-upload": "^1.2.7",
    "react-mobile-picker": "^0.1.12",
    "react-redux": "^6.0.1",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm -R ../marshal_app/www/* && cp -R build/* ../marshal_app/www/ && cd ../marshal_app && cordova build ios",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "copy-fonts": "cpy node_modules/framework7-icons/fonts/*.* src/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} src/fonts",
    "postinstall": "npm run copy-fonts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cpy-cli": "^2.0.0",
    "gh-pages": "^2.1.1"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
